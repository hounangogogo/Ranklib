java -jar RankLib.jar -train /Users/zhaohaonan/NLP/Ranklib/data_set/Fold5/train.txt -ranker 8 -metric2t MAP -save /Users/zhaohaonan/NLP/Ranklib/data_set/Fold5/model.txt

java -jar RankLib.jar -load /Users/zhaohaonan/NLP/Ranklib/data_set/Fold5/model.txt -rank /Users/zhaohaonan/NLP/Ranklib/data_set/Fold5/test.txt -metric2T MAP -score 8_result.txt
